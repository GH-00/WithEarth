<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="-10dp"
        android:right="-10dp"
        android:top="-10dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="4dp"
                android:color="#EAEAEA"
                android:dashGap="4dp"
                android:dashWidth="5dp"/>
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>